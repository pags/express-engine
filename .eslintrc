{
    "root": true,
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-catch-shadow": 2,
        "comma-dangle": 2,
        "valid-jsdoc": [1, {"requireReturnDescription": false, "requireReturn": false, "requireParamDescription": false}],
        "new-cap": [2, {"capIsNew": false}],
        "no-floating-decimal": 2,
        "no-throw-literal": 2,
        "wrap-iife": 2,
        "strict": [1, "never"],
        "no-undefined": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "consistent-this": [2, "self"],
        "no-lonely-if": 2,
        "no-unneeded-ternary": 2,
        "operator-assignment": [2, "always"],
        "quotes": [2, "single"],
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-use-before-define": 1,
        "consistent-return": 1,
        "no-undef": 2,
        "no-cond-assign": 2,
        "no-console": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "functions"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-mixed-spaces-and-tabs": [2, false],
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-unused-expressions": 2,
        "dot-notation": 2,
        "no-alert": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-native-reassign": 2,
        "semi": 2
    }
}